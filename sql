CREATE TABLE `gfg`.`users` ( `user_id` INT NOT NULL AUTO_INCREMENT , `first_name` VARCHAR(20) NOT NULL , `middle_name` VARCHAR(20) NOT NULL , `last_name` VARCHAR(20) NOT NULL , `city` VARCHAR(20) NOT NULL , `state` VARCHAR(20) NOT NULL , `zipcode` CHAR(6) NOT NULL , `email` VARCHAR(20) NOT NULL , `dob` DATE NOT NULL , `biodata` TEXT NOT NULL , `password` VARCHAR(20) NOT NULL , `user_type` VARCHAR NOT NULL DEFAULT 'user' , PRIMARY KEY (`user_id`)) ENGINE = InnoDB; 